generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model eecs {
  course_title                             String          @db.Text
  course_code                              String          @id @unique(map: "course_code_UNIQUE") @db.VarChar(45)
  prerequisites_eecsToprerequisites_course prerequisites[] @relation("eecsToprerequisites_course")
  prerequisites_eecsToprerequisites_prereq prerequisites[] @relation("eecsToprerequisites_prereq")
}

model prerequisites {
  course                          String @db.VarChar(45)
  prereq                          String @db.VarChar(45)
  eecs_eecsToprerequisites_course eecs   @relation("eecsToprerequisites_course", fields: [course], references: [course_code], onDelete: NoAction, onUpdate: NoAction, map: "course_fk")
  eecs_eecsToprerequisites_prereq eecs   @relation("eecsToprerequisites_prereq", fields: [prereq], references: [course_code], onDelete: Cascade, map: "prereq_fk")

  @@id([course, prereq])
  @@index([prereq], map: "prereq_fk_idx")
}